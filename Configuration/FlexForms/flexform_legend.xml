<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>Settings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.showAllCategories>
						<TCEforms>
							<label>Show events of all categories</label>
							<config>
								<type>check</type>
								<renderType>checkboxToggle</renderType>
							</config>
						</TCEforms>
					</settings.showAllCategories>
					<settings.showCategory>
						<TCEforms>
							<label>Event categories to be shown (if not all)</label>
							<config>
								<type>select</type>
								<renderType>selectMultipleSideBySide</renderType>
								<foreign_table>tx_fkuagenda_domain_model_category</foreign_table>
								<size>5</size>
								<autoSizeMax>20</autoSizeMax>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</settings.showCategory>
					<settings.monthlyListPid>
						<TCEforms>
							<label>Monthly view page</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</settings.monthlyListPid>
					<settings.editPid>
						<TCEforms>
							<label>Admin/edit page</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</settings.editPid>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>